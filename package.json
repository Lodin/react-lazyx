{
  "name": "react-lazyx",
  "version": "0.1.0",
  "description": "React bindings for Lazyx",
  "main": "./commonjs/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "types": "es/index.d.ts",
  "scripts": {
    "clean": "rimraf commonjs es umd coverage",
    "lint": "npm-run-all --parallel lint:*",
    "lint:src": "tslint \"src/**/*.+(ts|tsx)\" --type-check -c tslint.json -p tsconfig.tslint.json -t verbose",
    "lint:spec": "tslint \"spec/**/*.+(ts|tsx)\" --type-check -c tslint.json -p tsconfig.tslint.json -t verbose",
    "check-es3": "check-es3-syntax umd/ --patch && apply-patch react-lazyx.js.patch && rimraf react-lazyx.js.patch",
    "test": "nodemon --watch src --watch spec -e ts,tsx --exec \"yarn run test:single\"",
    "test:single": "jasmine --config=jasmine.json",
    "test:cover": "nyc yarn run test:single",
    "build:commonjs": "yarn run build:commonjs:files && yarn run build:commonjs:copydts",
    "build:commonjs:files": "tsc --outDir commonjs --target es5 -m commonjs --mapRoot ../commonjs --declaration",
    "build:commonjs:copydts": "cd src && copyfiles **/*.d.ts ../commonjs",
    "build:es": "yarn run build:es:files && yarn run build:es:copydts",
    "build:es:files": "tsc --outDir es --mapRoot ../es --declaration",
    "build:es:copydts": "cd src && copyfiles **/*.d.ts ../es",
    "build:umd": "cross-env NODE_ENV=development webpack --config=config/webpack.dev.js",
    "build:umd:min": "cross-env NODE_ENV=production webpack --config=config/webpack.prod.js",
    "build": "yarn run build:commonjs && yarn run build:es && yarn run build:umd && yarn run build:umd:min",
    "prepublish": "yarn run clean && yarn run lint && yarn run test:single && yarn run build && yarn run check-es3",
    "precommit": "lint-staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/poetez/react-lazyx.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "flux",
    "redux",
    "lazyx",
    "reactive"
  ],
  "author": "Vlad Rindevich <rindevich.vs@gmail.com> (https://github.com/Lodin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/poetez/react-lazyx/issues"
  },
  "homepage": "https://github.com/poetez/react-lazyx#readme",
  "devDependencies": {
    "@types/enzyme": "^2.8.0",
    "@types/jasmine": "^2.5.47",
    "@types/node": "^7.0.27",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^15.0.26",
    "apply-patch": "^0.1.2",
    "awesome-typescript-loader": "^3.1.3",
    "check-es3-syntax-cli": "^0.2.0",
    "codecov": "^2.2.0",
    "copyfiles": "^1.2.0",
    "cross-env": "^4.0.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.2.0",
    "husky": "^0.13.3",
    "jasmine": "^2.6.0",
    "jasmine-spec-reporter": "^4.1.0",
    "jsdom": "^11.0.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^3.4.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.3.2",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "ts-node": "^3.0.4",
    "tslint": "^5.2.0",
    "tslint-config-poetez": "^0.1.1",
    "typescript": "^2.3.2",
    "webpack": "^2.5.1"
  },
  "dependencies": {
    "prop-types": "^15.5.10",
    "tslib": ">=1.7.0"
  },
  "peerDependencies": {
    "lazyx": "^0.1.0",
    "react": "^15.0.0-0 || ^16.0.0-0",
    "rxjs": "^5.4.0"
  }
}
